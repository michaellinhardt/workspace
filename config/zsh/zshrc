reload () { source ~/.zshrc }

# EXPORTS
source ~/workspace/config/zsh/exports.zsh

# ZSH CONFIG
source $ZSH/oh-my-zsh.sh

# COLORS
source $ZSH_CONF/colors.zsh

# ALIAS
source $ZSH_CONF/alias.zsh

# GIT
source $ZSH_CONF/git.zsh

# SSH HOOKS
source $ZSH_CONF/ssh_with_hooks.zsh

# FORCE BG COLOR
source $ZSH_CONF/force_bgcolor.zsh
